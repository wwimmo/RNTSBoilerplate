{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": true,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "jsx-alignment": false,
    "jsx-curly-spacing": false
  }
}
